{
  "name": "@molty/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "prisma generate && tsx watch src/cli.ts",
    "build": "prisma generate && tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "start": "node dist/cli.js",
    "backfill:achievements": "tsx src/scripts/backfill-achievements.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "prisma generate && vitest run",
    "lint": "echo \"lint not configured\""
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/node": "^1.1.3",
    "elysia": "^1.1.12",
    "@sinclair/typebox": "^0.34.0",
    "@prisma/client": "^5.20.0",
    "dotenv": "^16.4.5",
    "pg": "^8.13.0",
    "@molty/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "prisma": "^5.20.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0"
  }
}
